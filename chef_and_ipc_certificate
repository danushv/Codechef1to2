/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner input=new Scanner(System.in);
		int n=input.nextInt();
		 int minimum =input.nextInt();
		 int l=input.nextInt();
		 int result=0;
		for(int t=0;t<n;t++)
		{
		   
		    int [] arr=new int[l];
		     int tot=0;
		    
		    for(int i=0;i<l;i++)
		    {
		        arr[i]=input.nextInt();
		        tot+=arr[i];
		        
		    }
		    int q=input.nextInt();
		    
		   
		    if(q<=10&& tot>=minimum)
		    {
		        result++;
		    }
		    
		    
		}
		System.out.println(result);
		
		
	}
}
